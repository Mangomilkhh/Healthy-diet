.select_wrap.data-v-2fe7f3f1 {
  flex: 1;
  display: inline-block;
  position: relative;
}
.select_wrap .select_input.data-v-2fe7f3f1 {
  -webkit-appearance: none;
  background-color: #fff;
  background-image: none;
  border-radius: 4px;
  border: 1px solid #dcdfe6;
  color: #606266;
  display: inline-block;
  font-size: inherit;
  height: 35px;
  line-height: 35px;
  outline: none;
  padding: 0 15px;
  box-sizing: border-box;
  transition: border-color 0.2s cubic-bezier(0.645, 0.045, 0.355, 1);
  width: 100%;
  padding-right: 30px;
}
.select_wrap .select_input .input_info.data-v-2fe7f3f1 {
  font-size: 14px;
  width: 100%;
  height: 100%;
}
.select_wrap .select_input .input_info .text_tips.data-v-2fe7f3f1 {
  height: 100%;
}
.select_wrap .select_input .input_info .text_tips text.data-v-2fe7f3f1 {
  font-size: 13px;
  color: #a0a9b0;
}
.select_wrap .select_input .icon_arrow.data-v-2fe7f3f1 {
  position: absolute;
  width: 30px;
  height: 40px;
  right: 0;
  top: 0;
  text-align: center;
  color: #c0c4cc;
  cursor: pointer;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 999;
}
.select_wrap .select_input .icon_arrow .arrow.data-v-2fe7f3f1 {
  width: 10px;
  height: 10px;
  background-color: transparent;
  /* 模块背景为透明 */
  border-color: #c0c4cc;
  border-style: solid;
  border-width: 1px 1px 0 0;
  transition: all 0.3s;
  box-sizing: border-box;
  /*箭头方向可以自由切换角度*/
}
.select_wrap .select_input .icon_arrow .arrow_down.data-v-2fe7f3f1 {
  transform: rotate(-45deg);
  margin-top: 5px;
}
.select_wrap .select_input .icon_arrow .arrow_up.data-v-2fe7f3f1 {
  transform: rotate(135deg);
  margin-top: -5px;
}
.select_wrap .select_input .icon_arrow .arrow-clear.data-v-2fe7f3f1 {
  width: 14px;
  height: 14px;
  border: 1px solid #e4e7ed;
  color: #e4e7ed;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 12px;
}
.select_wrap .select_input_select.data-v-2fe7f3f1 {
  border-color: #409eff;
}
.select_modal_con.data-v-2fe7f3f1 {
  width: 100%;
  transform-origin: center top;
  z-index: 2062;
  position: absolute;
  top: 40px;
  left: 0;
  border: 1px solid #e4e7ed;
  border-radius: 4px;
  background-color: #fff;
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
  box-sizing: border-box;
  margin-top: 12px;
}
.select_modal_con .cons_arrow.data-v-2fe7f3f1 {
  position: absolute;
  display: block;
  width: 0;
  height: 0;
  border-color: transparent;
  border-style: solid;
  top: -6px;
  left: 10%;
  margin-right: 3px;
  border-top-width: 0;
  border-bottom-color: #ebeef5;
}
.select_modal_con .cons_arrow.data-v-2fe7f3f1:after {
  content: " ";
  border-width: 6px;
  position: absolute;
  display: block;
  width: 0;
  height: 0;
  border-color: transparent;
  border-style: solid;
  top: 1px;
  margin-left: -6px;
  border-top-width: 0;
  border-bottom-color: #fff;
}
.select_modal.data-v-2fe7f3f1 {
  overflow: scroll;
  height: 160px;
}
.select_modal .select_content.data-v-2fe7f3f1 {
  list-style: none;
  padding: 6px 0;
  margin: 0;
  box-sizing: border-box;
}
.select_modal .select_content .select_content_li.data-v-2fe7f3f1 {
  font-size: 13px;
  padding: 0 20px;
  position: relative;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #606266;
  height: 36px;
  line-height: 36px;
  box-sizing: border-box;
  cursor: pointer;
}
.select_modal .select_content .select_content_li.selected.data-v-2fe7f3f1 {
  color: #409eff;
  font-weight: 700;
  background-color: #f5f7fa;
}
.select_modal .select_content .select_content_li.data-v-2fe7f3f1:hover {
  background-color: #f5f7fa;
}
.select_modal_scroll.data-v-2fe7f3f1 {
  overflow: hidden;
  height: 160px;
}
